<%= form_for [@project, @ticket] do |f| %>
  <%= f.error_messages %>

  <div class="field">
    <%= f.label :title, "Title:" %><br />
    <%= f.text_field :title %>
  </div>
  
  <div class="field">
    <%= f.label :description, "Description:" %><br />
    <%= f.text_field :description %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
